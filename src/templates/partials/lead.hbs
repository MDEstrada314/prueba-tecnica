<div class="contenido">
  <div class="com1">
    <img src="{{lead.[0].image.src}}" alt="{{pageLead.[0].alt}}">

  </div>
  <div class="com2">
    <div class="headline">
      <h1>{{headline}}</h1>
    </div>
    <div class="subheadllne ">
      <h6>{{subHeadline}}</h6>
    </div>

    <div class="audio-player">
      <div class="div-audio">
        <h3>{{audioPlayer.[0].audioTitle}}</h3>
        <h4>bumbox</h4>
        <div class="div-time">
          <span id="current-time">0:00</span>
          <span id="duration">0:00</span>
        </div>
        <input type="range" id="seek-bar" class="range" value="0">
      </div>

      <div class="div-play-wave">
        <button id="play-pause">
          <img src="https://img.icons8.com/?size=100&id=397&format=png&color=000000" alt="">
        </button>

        <div class="wave-container">
          <div class="wave"></div>
          <div class="wave"></div>
          <div class="wave"></div>
        </div>



      </div>
      <audio id="audio" src="{{audioPlayer.[0].audioUrl}}"></audio>
    </div>


    <div class="action">
      <h6>Compartir:</h6>
      {{#each actions.[0].items}}
      <a href="{{url}}" target="_blank data-social-service=" facebook" title="facebook" type="text/html" data-cms-ai="0""> {{this.body}} </a>
   
      {{/each}}
    </div>
  </div>
</div>